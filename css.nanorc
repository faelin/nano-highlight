syntax "css" "\.css$"

~OPERATOR: ":not\([A-Za-z][A-Za-z0-9]*\)"
PLAIN:     start="\{" end="\}"

#
# CSS3 Selectors
#
~TAG:       "(^|\*/|[][:space:],}>+~()])(a|abbr|acronym|address|area|article|applet|aside)([[:space:],#.{[>+~:)]|/\*|$)"
~TAG:       "(^|\*/|[][:space:],}>+~()])(audio|b|base|bd[io]|bgsound|big|blink|blockquote)([[:space:],#.{[>+~:)]|/\*|$)"
~TAG:       "(^|\*/|[][:space:],}>+~()])(body|(no)?br|button|canvas|caption|center|cite|code)([[:space:],#.{[>+~:)]|/\*|$)"
~TAG:       "(^|\*/|[][:space:],}>+~()])((col)?group|command|content|datalist|d[dlt]|del)([[:space:],#.{[>+~:)]|/\*|$)"
