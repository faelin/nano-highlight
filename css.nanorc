~TAG:       "(^|\*/|[][:space:],}>+~()])(title|track|tt|u|var|video|wbr|xmp)([[:space:],#.{[>+~:)]|/\*|$)"
~TAG:       "(^|[][:space:],}])\*([[:space:],{:[]|$)"

OPERATOR:   "[[:space:]]*[>+~][[:space:]]*"
~OPERATOR:  ":(active|checked|(dis|en)abled|empty|(first|last|only)-(child|of-type)|focus|hover|in-range|link)\>"
~OPERATOR:  ":(optional|out-of-range|read-(only|write)|required|root|selection|target|visited|(in)?valid|visited)\>"
VARIABLE:   "[.#][A-Za-z_][A-Za-z0-9_-]*"
OPERATOR:   "::(after|before|first-(letter|line)|selection)\>"
color red   "\!important[[:space:]]*(;|/\*|$)"

#
# CSS3 Keywords (predefined property-values mostly)
#
SECTION:    "(^|}|\*/)[[:space:]]*@(font-face|media)\>"
KEYWORD:    "\<(above|absolute|always|armenian|auto|avoid|baseline|behind|below|bidi-override|blink)([[:space:];,]|/\*|$)"
KEYWORD:    "\<(block|bold(er)?|bottom|break-word|capitalize|center(-left|-right)?|circle|collapse)([[:space:];,]|/\*|$)"
KEYWORD:    "\<(cjk-ideographic|(no-)?(close|open)-quote|compact|continuous|crop|cross(hair)?)([[:space:];,]|/\*|$)"
KEYWORD:    "\<((extra-|semi-|ultra-)?(condensed|expanded)|cursive|dashed|default)([[:space:];,]|/\*|$)"
KEYWORD:    "\<(decimal(-leading-zero)?|digits|disc|dotted|double|embed|fantasy|far-(left|right))([[:space:];,]|/\*|$)"
KEYWORD:    "\<(fast(er)?|fixed|georgian|groove|hebrew|help|hide|high(er)?|(hirag|katak)ana(-iroha)?)([[:space:];,]|/\*|$)"
KEYWORD:    "\<(inherit|initial|inline(-block|-table)?|(in|out)(set|side)|invert|italic|justify|landscape|larger?)([[:space:];,]|/\*|$)"
KEYWORD:    "\<((left|right)(-side|wards)?|level|lighter|line-through|list-item|loud|low(er)?|ltr)([[:space:];,]|/\*|$)"
KEYWORD:    "\<((lower|upper)(-alpha|-greek|-latin|-roman|case)|marker|medium|middle|mix|monospace)([[:space:];,]|/\*|$)"
KEYWORD:    "\<(move|narrower|none|normal|(no|pre-)wrap|oblique|once|overline|pointer)([[:space:];,]|/\*|$)"
KEYWORD:    "\<(portait|relative|(no-)?repeat|repeat-[xy]|([ew]|[ns][ew]?)-resize|ridge|rlt|run-in)([[:space:];,]|/\*|$)"
KEYWORD:    "\<(scroll|separate|(sans-)?serif|show|silent|slow(er)?|small(er)?|small-caps|soft|solid)([[:space:];,]|/\*|$)"
KEYWORD:    "\<(spell-out|square|static|sub|super|text(-bottom|-top)?|thick|thin|top)([[:space:];,]|/\*|$)"
KEYWORD:    "\<(table-(caption|cell|(column|row)(-group)?|(footer|header)-group)|underline|unset|visible)([[:space:];,]|/\*|$)"
KEYWORD:    "\<(wait|wider|x-(fast|high|loud|s?low|soft)|xx?-(large|small))([[:space:];,]|/\*|$)"

