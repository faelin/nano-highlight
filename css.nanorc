~TAG:       "(^|\*/|[][:space:],}>+~()])(fieldset|figcaption|figure|(base)?font|footer|form)([[:space:],#.{[>+~:)]|/\*|$)"
~TAG:       "(^|\*/|[][:space:],}>+~()])(frame(set)?|h[1-6]|head(er)?|hgroup|hr|html|i|iframe)([[:space:],#.{[>+~:)]|/\*|$)"
~TAG:       "(^|\*/|[][:space:],}>+~()])(img|input|ins|isindex|kbd|keygen|label|legend|li)([[:space:],#.{[>+~:)]|/\*|$)"
~TAG:       "(^|\*/|[][:space:],}>+~()])(link|listing|main|map|mark|marquee|menu(item)?|meta)([[:space:],#.{[>+~:)]|/\*|$)"
~TAG:       "(^|\*/|[][:space:],}>+~()])(meter|nav|noframes|object|[ou]l|optgroup|option)([[:space:],#.{[>+~:)]|/\*|$)"
~TAG:       "(^|\*/|[][:space:],}>+~()])(output|p|param|plaintext|pre|progress|q|r[pt]|ruby|s)([[:space:],#.{[>+~:)]|/\*|$)"
~TAG:       "(^|\*/|[][:space:],}>+~()])(samp|(no)?script|section|select|shadow|small|source)([[:space:],#.{[>+~:)]|/\*|$)"
~TAG:       "(^|\*/|[][:space:],}>+~()])(spacer|span|strike|strong|style|su[bp]|summary|table)([[:space:],#.{[>+~:)]|/\*|$)"
