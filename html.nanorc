syntax "html" "\.html?$"
header "<!doctype html"

# Everything inside angle brackets is erroneous initially
# This will be fixed later by correct syntax
WARNING:    start="<" end=">"
# All ampersands are initially erroneous too
WARNING:    "&"


#
# Attributes
#

# Explicitly-erroneous attributes
~WARNING:   "[[:space:]]*\<(pubdate|seamless)[[:space:]]*([=>]|$)"
# Global attributes
~ATTRIBUTE: "[[:space:]]*\<(accesskey|contenteditable|contextmenu|dir|draggable|dropzone)[[:space:]]*([=>]|$)"
~ATTRIBUTE: "[[:space:]]*\<(hidden|id|lang|spellcheck|style|tabindex|title|translate)[[:space:]]*([=>]|$)"
ATTRIBUTE:  "[[:space:]]*\<[Dd][Aa][Tt][Aa]-[a-z0-9_.]+(-[a-z0-9_.]+)*[[:space:]]*([=>]|$)"
# Element-specific attributes
~ATTRIBUTE: "[[:space:]]*\<(accept(-charset)?|action|alt|async|auto(complete|focus|play)|challenge)[[:space:]]*([=>]|$)"
~ATTRIBUTE: "[[:space:]]*\<(charset|checked|cite|class|(col|row)(s|span)|content|controls|coords)[[:space:]]*([=>]|$)"
~ATTRIBUTE: "[[:space:]]*\<(crossorigin|data|datetime|default|defer|dirname|disabled|download)[[:space:]]*([=>]|$)"
~ATTRIBUTE: "[[:space:]]*\<(enctype|for|form(action)?|headers|height|high|href(lang)?|http-equiv)[[:space:]]*([=>]|$)"
~ATTRIBUTE: "[[:space:]]*\<(icon|ismap|keytype|kind|label|list|loop|low|manifest|max(length)?)[[:space:]]*([=>]|$)"
~ATTRIBUTE: "[[:space:]]*\<(media|method|min|multiple|muted|name|novalidate|open|optimum|pattern)[[:space:]]*([=>]|$)"
~ATTRIBUTE: "[[:space:]]*\<(placeholder|poster|preload|radiogroup|readonly|rel|required|reversed)[[:space:]]*([=>]|$)"
~ATTRIBUTE: "[[:space:]]*\<(sandbox|scoped?|selected|shape|sizes?|span|src(doc|lang)?|start|step)[[:space:]]*([=>]|$)"
~ATTRIBUTE: "[[:space:]]*\<(target|type(mustmatch)?|usemap|value|width|wrap)[[:space:]]*([=>]|$)"
# Attributes deprecated / not supported in HTML5
~PLAIN:     "[[:space:]]*\<(align|bgcolor|border|color|mediagroup)[[:space:]]*([=>]|$)"
# Event attributes
~FUNCTION:  "[[:space:]]*\<on(abort|afterprint|before(print|unload)|blur|canplay(through)?)[[:space:]]*(=|$)"
~FUNCTION:  "[[:space:]]*\<on((cue|duration|hash|rate|volume)?change|(dbl)?click|contextmenu|copy)[[:space:]]*(=|$)"
~FUNCTION:  "[[:space:]]*\<on(cut|drag(end|enter|leave|over|start)?|drop|emptied|ended|error|focus)[[:space:]]*(=|$)"
~FUNCTION:  "[[:space:]]*\<on(input|invalid|key(down|press|up)|load(ed(meta)?data|start)?)[[:space:]]*(=|$)"
~FUNCTION:  "[[:space:]]*\<on(mouse(down|move|out|over|up|wheel)|(off|on)line|page(hide|show))[[:space:]]*(=|$)"
~FUNCTION:  "[[:space:]]*\<on(paste|pause|play(ing)?|popstate|progress|reset|resize|scroll|search)[[:space:]]*(=|$)"
~FUNCTION:  "[[:space:]]*\<on(seek(ed|ing)|select|show|stalled|storage|submit|suspend|timeupdate)[[:space:]]*(=|$)"
~FUNCTION:  "[[:space:]]*\<on(toggle|unload|waiting|wheel)[[:space:]]*(=|$)"


#
# Elements
#

# Obsolete elements
WARNING:    "</?(bgsound|blink|command|eventsource|listing|nobr|plaintext|spacer|xmp)([[:space:]]+>?|>|[[:space:]]*$)"
# Normal elements valid in HTML5
~TAG:       "</?(a|abbr|address|article|aside|audio|b|bd[io]|blockquote|body|button)([[:space:]]+>?|>|[[:space:]]*$)"
~TAG:       "</?(canvas|caption|cite|code|colgroup|datalist|d[dlt]|del|details|dfn)([[:space:]]+>?|>|[[:space:]]*$)"
~TAG:       "</?(dialog|div|em|fieldset|figcaption|figure|footer|form|h[1-6]|head(er)?)([[:space:]]+>?|>|[[:space:]]*$)"
~TAG:       "</?(hgroup|html|i|iframe|ins|kbd|label|legend|li|main|map|mark|menu|meter)([[:space:]]+>?|>|[[:space:]]*$)"
~TAG:       "</?(nav|object|[ou]l|optgroup|option|output|p|pre|progress|q|r[pt]|ruby|s)([[:space:]]+>?|>|[[:space:]]*$)"
~TAG:       "</?(samp|(no)?script|section|select|small|span|strong|style|su[bp])([[:space:]]+>?|>|[[:space:]]*$)"
~TAG:       "</?(summary|table|t(body|foot|head)|t[dhr]|template|textarea|time|title)([[:space:]]+>?|>|[[:space:]]*$)"
~TAG:       "</?(u|var|video)([[:space:]]+>?|>|[[:space:]]*$)"
# Elements for which end tag is forbidden
~TAG:       "<(area|base|br|col|embed|hr|img|input|keygen)([[:space:]]+>?|>|[[:space:]]*$)"
~TAG:       "<(link|menuitem|meta|param|source|track|wbr)([[:space:]]+>?|>|[[:space:]]*$)"
# Elements deprecated in HTML5
~PLAIN:     "</?(acronym|applet|big|center|content|dir|(base)?font)([[:space:]]+>?|>|[[:space:]]*$)"
~PLAIN:     "</?(frame(set)?|isindex|marquee|noframes|shadow|strike|tt)([[:space:]]+>?|>|[[:space:]]*$)"

#
# Strings and values
#
STRING:     "=[[:space:]]*[]A-Za-z0-9!#$%()*+,./:;?@[\^_{|}~-][]A-Za-z0-9!"#$%'()*+,./:;?@[\^_{|}~-]*"
OPERATOR:   "="
+WHITESPACES
STRING:     ""[^"]*"|'[^']*'"
PLAIN:      start=">" end="<"
TAG:        "[<>]"

# Embedded template languages
VARIABLE:   "\{\{[^}]*\}\}|<%=.*%>"
SECTION:    start="<\?" end="\?>"
SECTION:    start="<%[^=]" end="%>"
SECTION:    start="\{%" end="%\}"

#
# Finally, character references and comments
#
WARNING:    "&[A-Za-z][A-Za-z0-9]*;"
COMMENT:    start="<!--" end="-->"
COMMENT:    "<!(doctype|DOCTYPE)[^>]*>"
CHARREF:    "&#([0-9]+|[Xx][0-9A-Fa-f]+);"
